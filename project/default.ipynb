   "source": [
    "text = \"France is ‘ hunting down its citizens who joined Isis Republic’ without trial in Iraq\"\n",
    "\n",
    "import nltk"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "tokens = nltk.word_tokenize(text)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "tagged = nltk.pos_tag(tokens)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(S\n",
      "  (GPE France/NNP)\n",
      "  is/VBZ\n",
      "  ‘/JJ\n",
      "  hunting/VBG\n",
      "  down/RP\n",
      "  its/PRP$\n",
      "  citizens/NNS\n",
      "  who/WP\n",
      "  joined/VBD\n",
      "  (PERSON Isis/NNP Republic/NNP)\n",
      "  ’/NNP\n",
      "  without/IN\n",
      "  trial/NN\n",
      "  in/IN\n",
      "  (GPE Iraq/NNP))\n"
     ]
    }
   ],
   "source": [
    "entities = nltk.chunk.ne_chunk(tagged)\n",
    "print(entities)"
   ]
